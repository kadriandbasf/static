	{ "keys": [ "ctrl+alt+j" ], "command": "pretty_json" },
	{ "keys": ["ctrl+shift+o"], "command": "prompt_open_folder" },
	{ "keys": ["ctrl+alt+c"], "command": "copy_path" },
